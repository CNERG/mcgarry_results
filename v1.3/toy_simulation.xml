<simulation>
  <control>
    <duration>5</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib><name>Source</name> </spec>
    <spec> <lib>cycamore</lib><name>Sink</name> </spec>
    <spec> <lib>cycamore</lib><name>Enrichment</name> </spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
 </archetypes>

  <facility>
    <name>Mine</name>
    <config>
      <Source>
        <outcommod>nat_uranium</outcommod>
        <outrecipe>nat_u_recipe</outrecipe>
        <throughput>1000.0</throughput>
      </Source>
    </config>
  </facility>

  <facility>
    <name>LEU_Requestor</name>
    <config>
      <Sink>
        <in_commods>
	  <val>leu</val>
	</in_commods>
	<recipe_name>leu_recipe</recipe_name>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Enrichment</name>
    <config>
      <Enrichment>
        <feed_commod>nat_uranium</feed_commod>
	<feed_recipe>nat_u_recipe</feed_recipe>
	<tails_commod>tails</tails_commod>
	<tails_assay>0.003</tails_assay>
	<product_commod>leu</product_commod>
	<swu_capacity>180.0</swu_capacity>
       </Enrichment>
    </config>
  </facility>

  <recipe>
    <name>nat_u_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.0071</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.993</comp> </nuclide>
  </recipe>
 
 <recipe>
    <name>leu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.04</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.96</comp> </nuclide>
  </recipe>

  <region>
   <name>SingleRegion</name>
   <config><NullRegion/></config>
   <institution>
     <name>SingleInstitution</name>
     <initialfacilitylist>
       <entry>
	 <prototype>Mine</prototype>
	 <number>1</number>
       </entry>
       <entry>
	 <prototype>Enrichment</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>LEU_Requestor</prototype>
	 <number>1</number>
       </entry>
     </initialfacilitylist>
     <config><NullInst/></config>
   </institution>
 </region>

 
</simulation>