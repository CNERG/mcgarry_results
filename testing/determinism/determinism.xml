<simulation>
  <control>
    <duration>50</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib><name>Source</name> </spec>
    <spec> <lib>cycamore</lib><name>Sink</name> </spec>
    <spec> <lib>cycamore</lib><name>EnrichmentFacility</name> </spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
 </archetypes>

  <facility>
    <name>Source</name>
    <config>
      <Source>
        <out_commod>nat_uranium</out_commod>
        <recipe_name>nat_u_recipe</recipe_name>
        <capacity>1000.0</capacity>
      </Source>
    </config>
  </facility>

  <facility>
    <name>LEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>leu</val>
	</in_commods>
	<recipe_name>leu_recipe</recipe_name>
	<social_behav>None</social_behav>
	<user_pref>6</user_pref>
	<avg_qty>33</avg_qty>
	<sigma>0.5</sigma>
	<rng_seed>0</rng_seed>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>delta_LEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>heu</val>
	</in_commods>
	<recipe_name>heu_recipe</recipe_name>
	<avg_qty>1.0</avg_qty>
	<sigma>0</sigma>
	<social_behav>None</social_behav>
	<user_pref>1</user_pref>
      </Sink>
    </config>
  </facility>

 <facility>
    <name>covert_HEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>heu</val>
	</in_commods>
	<recipe_name>heu_recipe</recipe_name>
	<avg_qty>0.03</avg_qty>
	<sigma>0</sigma>
	<social_behav>Every</social_behav>
	<behav_interval>2</behav_interval>
	<user_pref>8</user_pref>
	<rng_seed>0</rng_seed>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>EnrichmentFacility</name>
    <config>
      <EnrichmentFacility>
        <in_commod>nat_uranium</in_commod>
	<in_recipe>nat_u_recipe</in_recipe>
	<tails_assay>0.003</tails_assay>
	<out_commods>
          <val>leu</val>
          <val>heu</val>
	</out_commods>
	<swu_capacity>180.0</swu_capacity>
       </EnrichmentFacility>
    </config>
  </facility>

  <recipe>
    <name>nat_u_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.0071</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.993</comp> </nuclide>
  </recipe>
 
 <recipe>
    <name>leu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.04</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.96</comp> </nuclide>
  </recipe>
 <recipe>
    <name>heu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.90</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.10</comp> </nuclide>
  </recipe>


  <region>
   <name>SingleRegion</name>
   <config><NullRegion/></config>
   <institution>
     <name>SingleInstitution</name>
     <initialfacilitylist>
       <entry>
	 <prototype>Source</prototype>
	 <number>1</number>
       </entry>
       <entry>
	 <prototype>EnrichmentFacility</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>LEU</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>delta_LEU</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>covert_HEU</prototype>
	 <number>1</number>
       </entry>
     </initialfacilitylist>
     <config><NullInst/></config>
   </institution>
 </region>

 
</simulation>