<simulation>
  <control>
    <duration>50</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib><name>Source</name> </spec>
    <spec> <lib>cycamore</lib><name>Sink</name> </spec>
    <spec> <lib>cycamore</lib><name>Enrichment</name> </spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
 </archetypes>

  <facility>
    <name>Mine</name>
    <config>
      <Source>
        <outcommod>nat_uranium</outcommod>
        <outrecipe>nat_u_recipe</outrecipe>
        <throughput>1000.0</throughput>
      </Source>
    </config>
  </facility>

  <facility>
    <name>LEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>enriched</val>
	</in_commods>
	<recipe_name>leu_recipe</recipe_name>
	<social_behav>None</social_behav>
	<avg_qty>33</avg_qty>
	<sigma>0.5</sigma>
	<rng_seed>0</rng_seed>
	<user_pref>6</user_pref>
       </Sink>
    </config>
  </facility>

<facility>
    <name>delta_LEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>enriched</val>
	</in_commods>
	<recipe_name>leu_recipe</recipe_name>
	<avg_qty>10.0</avg_qty>
	<sigma>0</sigma>
	<social_behav>None</social_behav>
	<user_pref>1</user_pref>
      </Sink>
    </config>
  </facility>

  
 <facility>
    <name>covert_HEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>enriched</val>
	</in_commods>
	<recipe_name>heu_recipe</recipe_name>
	<avg_qty>0.03</avg_qty>
	<sigma>0</sigma>
	<social_behav>Reference</social_behav>
	<behav_interval>2</behav_interval>
	<rng_seed>0</rng_seed>
	<user_pref>8</user_pref>
      </Sink>
    </config>
  </facility>
  
  <facility>
    <name>Enrichment</name>
    <config>
      <Enrichment>
        <feed_commod>nat_uranium</feed_commod>
	<feed_recipe>nat_u_recipe</feed_recipe>
	<tails_commod>tails</tails_commod>
	<tails_assay>0.003</tails_assay>
	<product_commod>enriched</product_commod>
	<swu_capacity>180.0</swu_capacity>
       </Enrichment>
    </config>
  </facility>

  <recipe>
    <name>nat_u_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.0071</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.993</comp> </nuclide>
  </recipe>
 
 <recipe>
    <name>leu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.04</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.96</comp> </nuclide>
  </recipe>
 <recipe>
    <name>heu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.90</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.10</comp> </nuclide>
  </recipe>

  
  <region>
   <name>SingleRegion</name>
   <config><NullRegion/></config>
   <institution>
     <name>SingleInstitution</name>
     <initialfacilitylist>
       <entry>
	 <prototype>Mine</prototype>
	 <number>1</number>
       </entry>
       <entry>
	 <prototype>Enrichment</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>LEU</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>delta_LEU</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>covert_HEU</prototype>
	 <number>1</number>
       </entry>
     </initialfacilitylist>
     <config><NullInst/></config>
   </institution>
 </region>

 
</simulation>