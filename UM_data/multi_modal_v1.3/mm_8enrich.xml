<simulation>
  <control>
    <duration>2000</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib><name>Source</name> </spec>
    <spec> <lib>cycamore</lib><name>Sink</name> </spec>
    <spec> <lib>cycamore</lib><name>Enrichment</name> </spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
 </archetypes>

  <facility>
    <name>Mine</name>
    <config>
      <Source>
        <outcommod>nat_uranium</outcommod>
        <outrecipe>nat_u_recipe</outrecipe>
        <throughput>1000.0</throughput>
      </Source>
    </config>
  </facility>

  <facility>
    <name>LEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>enriched</val>
	</in_commods>
        <recipe_names>
	  <val>leu3_recipe</val>
	  <val>leu33_recipe</val>
	  <val>leu36_recipe</val>
	  <val>leu39_recipe</val>
	  <val>leu42_recipe</val>
	  <val>leu45_recipe</val>
	  <val>leu48_recipe</val>
	  <val>leu5_recipe</val>
	</recipe_names>
	<social_behav>None</social_behav>
	<avg_qty>33</avg_qty>
	<sigma>0.66</sigma>
	<rng_seed>1</rng_seed>
	<user_pref>6</user_pref>
       </Sink>
    </config>
  </facility>

 <facility>
    <name>covert_HEU</name>
    <config>
      <Sink>
        <in_commods>
	  <val>enriched</val>
	</in_commods>
	<recipe_name>heu_recipe</recipe_name>
	<avg_qty>0.03</avg_qty>
	<sigma>0</sigma>
	<social_behav>Random</social_behav>
	<behav_interval>5</behav_interval>
	<rng_seed>1</rng_seed>
	<user_pref>8</user_pref>
	<t_trade>1000</t_trade>
      </Sink>
    </config>
  </facility>
  
  <facility>
    <name>Enrichment</name>
    <config>
      <Enrichment>
        <feed_commod>nat_uranium</feed_commod>
	<feed_recipe>nat_u_recipe</feed_recipe>
	<tails_commod>tails</tails_commod>
	<tails_assay>0.002</tails_assay>
	<sigma_tails>0.001</sigma_tails>
	<product_commod>enriched</product_commod>
	<social_behav>None</social_behav>
	<rng_seed>1</rng_seed>
       </Enrichment>
    </config>
  </facility>

  <recipe>
    <name>nat_u_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.0071</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.993</comp> </nuclide>
  </recipe>
 
 <recipe>
    <name>leu3_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.03</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.97</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu33_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.033</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.967</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu36_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.036</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.964</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu39_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.039</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.961</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu42_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.042</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.958</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu45_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.045</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.955</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu48_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.048</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.952</comp> </nuclide>
  </recipe>
 <recipe>
    <name>leu5_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.05</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.95</comp> </nuclide>
  </recipe>
 <recipe>
    <name>heu_recipe</name>
    <basis>atom</basis>
    <nuclide> <id>922350000</id> <comp>0.90</comp> </nuclide>
    <nuclide> <id>922380000</id> <comp>0.10</comp> </nuclide>
  </recipe>

  
  <region>
   <name>SingleRegion</name>
   <config><NullRegion/></config>
   <institution>
     <name>SingleInstitution</name>
     <initialfacilitylist>
       <entry>
	 <prototype>Mine</prototype>
	 <number>1</number>
       </entry>
       <entry>
	 <prototype>Enrichment</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>LEU</prototype>
	 <number>1</number>
       </entry>
       <entry>
 	 <prototype>covert_HEU</prototype>
	 <number>1</number>
       </entry>
     </initialfacilitylist>
     <config><NullInst/></config>
   </institution>
 </region>

 
</simulation>